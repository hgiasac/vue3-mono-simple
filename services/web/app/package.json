{
  "name": "web",
  "version": "0.0.1",
  "description": "A demo vue3 app",
  "productName": "Sample App",
  "author": "Toan Nguyen <hgiasac@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "quasar dev",
    "serve:electron": "quasar dev -m electron",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:electron": "quasar build -m electron",
    "test:unit": "jest --updateSnapshot",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "lint": "eslint --ext .js,.ts,.tsx,.vue ./src",
    "format": "eslint --fix --ext .js,.ts,.tsx,.vue ./src",
    "generate-graphql-types": "rm -rf app/src/api/graphql/generated && graphql-codegen --config codegen.yml",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "firebase:deploy:dev": "firebase deploy --only hosting"
  },
  "dependencies": {
    "@hgiasac/quasar-wrapper": "^0.0.3",
    "@quasar/extras": "^1.10.9",
    "@urql/exchange-graphcache": "^4.2.1",
    "@urql/vue": "^0.4.2",
    "core-js": "^3.15.2",
    "graphql": "^15.5.1",
    "quasar": "^2.0.3",
    "vue": "^3.1.5",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-vue-urql": "^1.0.3",
    "@quasar/app": "^3.0.3",
    "@quasar/cli": "^1.2.1",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^3.1.0",
    "@quasar/quasar-app-extension-testing-quality": "^1.0.0-beta.8",
    "@quasar/quasar-app-extension-testing-security": "^1.0.0-alpha.8",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.2",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cypress": "^8.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "last 10 Chrome versions",
